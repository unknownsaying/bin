Imports System.Math
Dim t1 As Async  
Dim t2 As Await
Function t1t2
     t1[
       'a * sin(0 + t1)' + 'b * cos(0 - t2)',
       'c * sin(0 - t1)' + 'd * cos(0 + t2)'
    ]
     t2[
       'A * sin(www + t1)' + 'B * cos(www - t2)',
       'C * sin(www - t1)' + 'D * cos(www + t2)'
    ]
End Function