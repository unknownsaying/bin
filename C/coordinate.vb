Declare Module X
Declare Module Y
Declare Module Z
Function
    char radius,theta,fai;
    type X 
    typeof radius
    type Y 
    typeof radius * cos(theta)
    type Z 
    typeof radius * cos(theta) * sin(fai)
    type T 
    typeof radius * cos(theta) * cos(fai)
End Function
Sub loop
    char a,b,c,d,e,f;
   for(a=0;a<b;a++)
   for(b=00;b<c;b++)
   for(c=000;c<d;c++)
   for(d=000;d>e;--d)
   for(e=00;e>f;--e)
   for(f=0;f>a;--f)
End Sub

